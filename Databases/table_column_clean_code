	Ixodes_scapularis_data_source VARCHAR NOT NULL,
	Ixodes_pacificus_county_status VARCHAR NOT NULL,
	Ixodes_pacificus_data_source VARCHAR NOT NULL
);

SELECT * FROM tick_info_2021;

