CREATE TABLE tick_table_2020 (
	FIPSCode INT NOT NULL,
	State VARCHAR NOT NULL,
	County VARCHAR NOT NULL,
	Ixodes_scapularis_County_Status VARCHAR NOT NULL,
	Ixodes_scapularis_data_source VARCHAR NOT NULL,
	Ixodes_pacificus_county_status VARCHAR NOT NULL,
	Ixodes_pacificus_data_source VARCHAR NOT NULL
);

SELECT * FROM tick_table_2020;

CREATE TABLE tick_table_2021 (
	FIPSCode INT NOT NULL,
